<% provide(:title, 'Sign Up') %>

<h1>Sign Up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>

	    <%= render 'shared/error_messages' %>

		<%= render 'fields', f: f %>

	    <%= f.submit 'Create my account', class: 'btn btn-primary btn-lg' %>
    <% end %>
  </div>
</div>
